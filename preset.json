{
    "useConfigFiles": true,
    "plugins": {
        "@vue/cli-plugin-babel": {},
        "@vue/cli-plugin-pwa": {},
        "@vue/cli-plugin-eslint": {
            "config": "base",
            "lintOn": [
                "save"
            ]
        },
        "vue-cli-plugin-vuetify": {
            "version": "0.5.0",
            "preset": "configure",
            "replaceComponents": false,
            "useTheme": false,
            "useCustomProperties": true,
            "iconFont": "fa",
            "installFonts": false,
            "useAlaCarte": false,
            "usePolyfill": true,
            "locale": "nl"
        },
        "vue-cli-plugin-kingscode-scaffold": {
            "version": "0.2.0",
            "prompts": true
        }
    },
    "router": true,
    "routerHistoryMode": true,
    "vuex": true,
    "cssPreprocessor": "node-sass"
}
